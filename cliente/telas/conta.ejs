<%- include(process.cwd() + "/cliente/includes/header.ejs" ) %>



    <!-- Start Navbar Area -->
    <%- include(process.cwd() + "/cliente/includes/navbar.ejs" ) %>
        <!-- End Navbar Area -->


        <!-- Start Page Title -->
        <div class="page-title-area">
            <div class="container">
                <div class="page-title-content">
                    <h2>Minha Conta</h2>
                    <ul>
                        <li><a href="/">Inicio</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- End Page Title -->

        <!-- Start SignUP Area -->
        <section class="ptb-100">

            <div class="container">

                <div class="col-md-12 row">

                    <div class="col-lg-2">
                        <%- include(process.cwd() + "/cliente/includes/sidebarConta.ejs" ) %>
                    </div>


                    <div class="col-lg-10">
                        <div class="woocommerce-widget-area">
      
                           <div class="woocommerce-widget collections-list-widget">
                              <h3 class="woocommerce-widget-title">Meus Dados</h3>
      
                              <div class="row" id="row">
                                
                                <div class="col-lg-6 col-md-6">
                                    <div class="form-group">
                                       <label>Nome Completo <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.nome_completo %>">
                                    </div>
                                 </div>

                                 <div class="col-lg-3 col-md-3">
                                    <div class="form-group">
                                       <label>CPF <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.cpf %>">
                                    </div>
                                 </div>

                                 <div class="col-lg-3 col-md-3">
                                    <div class="form-group">
                                       <label>Data Nascimento <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.data_nascimento %>">
                                    </div>
                                 </div>
                                 
      
      
                              </div>
                              <hr/>
                              <div class="row" id="row">
                                
                                <div class="col-lg-6 col-md-6">
                                    <div class="form-group">
                                       <label>Email <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.email %>">
                                    </div>
                                 </div>

                                 <div class="col-lg-3 col-md-3">
                                    <div class="form-group">
                                       <label>Telefone <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.telefone %>">
                                    </div>
                                 </div>

                                 <div class="col-lg-3 col-md-3">
                                    <div class="form-group">
                                       <label>Celular <span class="required"id="reqSenha2">*</span></label>
                                       <input type="text" class="form-control" value="<%= sessao.conta.celular %>">
                                    </div>
                                 </div>
                                 
      
      
                              </div>
      
      
      
                           </div>
      
                        </div>
                     </div>
    
                </div>

            </div>

        </section>
        <!-- End SignUP Area -->



        <!-- Start Shopping Cart Modal -->
        <%- include(process.cwd() + "/cliente/includes/carrinho.ejs" ) %>
            <!-- End Shopping Cart Modal -->



            <!-- Start Footer Area -->
            <%- include(process.cwd() + "/cliente/includes/footer.ejs" , { scripts: [ `/cliente/js/entrar.js` ] } ) %>
                <!-- End Footer Area -->